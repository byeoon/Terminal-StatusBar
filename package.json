{
  "name": "terminal-statusbar",
  "displayName": "Terminal StatusBar",
  "description": "Adds a statusbar to directly access the terminal.",
  "publisher": "Byeoon",
  "version": "1.0.1",
  "repository": "https://github.com/byeoon/terminal-statusbar",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "terminal-statusbar.openTerminal",
        "title": "Open",
        "category": "Terminal StatusBar"
      },
      {
        "command": "terminal-statusbar.about",
        "title": "About",
        "category": "Terminal StatusBar"
      }
    ],
      "configuration": {
        "title": "Terminal StatusBar",
      "properties": {
        "terminal-statusbar.terminalArray": {
          "type": "integer",
          "default": 0,
          "description": "Determines which terminal to open. Use 0 for default."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
